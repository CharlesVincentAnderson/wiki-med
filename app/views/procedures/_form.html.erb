<%= form_for @procedure do |p| %>
  <%= p.text_field :name, placeholder: "Name"%>
  <%= p.date_field :date, placeholder: "Date"%>
  <%= p.number_field :price, placeholder: "Price", min:0, step:0.01%>
  <div>
    <%= collection_select(:procedure, :facility_id, Facility.all, :id, :name, prompt: true) %>
    <%= link_to  "Add Facility",new_facility_path %>
  </div>
  <%= p.submit "Submit"%>
<%end%>
